{
    "extends": "airbnb",
    "env": {
        "browser": true
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "indent": [1, 4], // warning, and 4 spaces
        "react/jsx-indent": [1, 4],
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
    },
    "parser": "babel-eslint"
}
